<AnchorPane id="AnchorPane" prefHeight="294.0" prefWidth="293.0" style="-fx-background-color: white;" stylesheets="@info_button.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="event.reminder.AlarmFXMLController">
   <children>
      <ImageView fitHeight="205.0" fitWidth="290.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/reminder-clipart-just-a-reminder-elephant-clipart.jpg" />
         </image>
      </ImageView>
      <Label fx:id="event_name" alignment="CENTER_RIGHT" layoutX="24.0" layoutY="196.0" prefHeight="40.0" prefWidth="84.0" text="Event Name" textFill="#4d08ee">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
      <Label layoutX="121.0" layoutY="205.0" prefHeight="23.0" prefWidth="26.0" text="at" textFill="#4d08ee">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
      <Label fx:id="event_date" layoutX="161.0" layoutY="196.0" prefHeight="40.0" prefWidth="132.0" text="Event Date" textFill="#4d08ee">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
      <Button fx:id="ok_button" layoutX="121.0" layoutY="236.0" mnemonicParsing="false" onAction="#handleOkButton" prefHeight="49.0" prefWidth="49.0" styleClass="okstyle" textFill="#4f0fee">
