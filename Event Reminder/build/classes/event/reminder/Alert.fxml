<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="164.0" prefWidth="244.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="event.reminder.AlertController">
   <children>
      <Label layoutX="21.0" layoutY="29.0" text="Type &quot;OK&quot; to delete all">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="text" layoutX="51.0" layoutY="69.0" />
      <Button fx:id="ok_button" layoutX="67.0" layoutY="115.0" mnemonicParsing="false" onAction="#handleOKbutton" text="OK" />
      <Button fx:id="cancel_button" layoutX="138.0" layoutY="115.0" mnemonicParsing="false" onAction="#handleCancelbutton" text="Cancel" />
   </children>
</AnchorPane>
